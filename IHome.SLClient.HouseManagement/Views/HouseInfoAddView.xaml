<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:IHome.SLClient.HouseManagement"
	x:Class="IHome.SLClient.HouseManagement.HoseInfoAddView"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:HouseInfoAddViewModel x:Key="HoseInfoAddViewModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource HoseInfoAddViewModelDataSource}}">
        <TextBlock Margin="122,109,289,193" TextWrapping="Wrap" Text="{Binding Result.data.MyProperty1, Mode=TwoWay}"/>
		<Button Content="提交" HorizontalAlignment="Right" Margin="0,0,156,60" Width="139" Height="120" VerticalAlignment="Bottom" Command="{Binding AddHoseInfoCommand, Mode=OneWay}"/>
	</Grid>
</UserControl>
