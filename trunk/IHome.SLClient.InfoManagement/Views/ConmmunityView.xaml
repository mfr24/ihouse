<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:IHome.SLClient.InfoManagement"
	xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
	mc:Ignorable="d"
	x:Class="IHome.SLClient.InfoManagement.ConmmunityView"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:ConmmunityViewModel x:Key="ConmmunityViewModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource ConmmunityViewModelDataSource}}">
		<telerik:RadGridView Margin="0,57,0,0"/>
		<Button Content="添加小区" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75"/>
	</Grid>
</UserControl>
