<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>ICommand MVVM</Title>
      <Shortcut>cmd</Shortcut>
      <Description>
      </Description>
      <Author>Gabry.Zhong</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>mywin</ID>
          <ToolTip>view_name</ToolTip>
          <Default>my</Default>
        </Literal>
        <Literal>
          <ID>title</ID>
          <ToolTip>title name</ToolTip>
          <Default>窗口名</Default>
        </Literal>
        <Literal>
          <ID>type</ID>
          <ToolTip>type_name</ToolTip>
          <Default>IHome.SLClient.InfoManagement.BuildingView</Default>
        </Literal>
        <Literal>
          <ID>xap</ID>
          <ToolTip>xap_name</ToolTip>
          <Default>IHome.SLClient.InfoManagement.zip</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp">
        <![CDATA[private IHome.Models.CmdWin _$mywin$View = new Models.CmdWin()
                {
                    name = "$title$",
                    type_name = "$type$",
                    xap_name = "$xap$",
                    Win = Models.CmdWin.WinType.modal,
                    VeiwModel = new BuildingViewModel()
                };
        public IHome.Models.CmdWin $mywin$View
        {
            get
            {
                return _$mywin$View;
            }
        }$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>