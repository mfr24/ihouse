<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:IHome.SLClient"
	x:Class="IHome.SLClient.NavigationView"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:NavigationViewModel x:Key="NavigationViewModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource NavigationViewModelDataSource}}">
		<Image Margin="0" Source="/IHome.SLClient;component/Images/navigation.png" Stretch="None" HorizontalAlignment="Center"/>
	</Grid>
</UserControl>
